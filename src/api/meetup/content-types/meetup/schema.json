{
  "kind": "collectionType",
  "collectionName": "meetups",
  "info": {
    "singularName": "meetup",
    "pluralName": "meetups",
    "displayName": "Meetup",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "dislay_name": {
